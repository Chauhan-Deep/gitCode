{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1, 
  "newProjectRoot": "projects",
  "projects": {
    "smart-content-model": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/smart-content-model",
      "sourceRoot": "projects/smart-content-model/src",
      "prefix": "smart-content-model",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/smart-content-model",
            "index": "projects/smart-content-model/src/SmartContentModel.html",
            "main": "projects/smart-content-model/src/main.ts",
            "polyfills": "projects/smart-content-model/src/polyfills.ts",
            "tsConfig": "projects/smart-content-model/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/smart-content-model/src/favicon.ico",
              "projects/smart-content-model/src/assets"
            ],
            "styles": [
              "node_modules/@quark/xpressng/styles/qx.css",
              "projects/smart-content-model/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/smart-content-model/src/environments/environment.ts",
                  "with": "projects/smart-content-model/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "smart-content-model:build",
            "port": 9001,
            "host": "0.0.0.0",
            "disableHostCheck": true
          },
          "configurations": {
            "production": {
              "browserTarget": "smart-content-model:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "smart-content-model:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/smart-content-model/src/test.ts",
            "polyfills": "projects/smart-content-model/src/polyfills.ts",
            "tsConfig": "projects/smart-content-model/tsconfig.spec.json",
            "karmaConfig": "projects/smart-content-model/karma.conf.js",
            "assets": [
              "projects/smart-content-model/src/favicon.ico",
              "projects/smart-content-model/src/assets"
            ],
            "styles": [
              "projects/smart-content-model/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/smart-content-model/tsconfig.app.json",
              "projects/smart-content-model/tsconfig.spec.json",
              "projects/smart-content-model/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/smart-content-model/e2e/protractor.conf.js",
            "devServerTarget": "smart-content-model:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "smart-content-model:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "smart-content-model"
}
